#include <stdio.h>
#include <conio.h>

int main()
{
    int n = 5, i, j, k;
    char ch=65;
    
    for(i = 0; i < n; i++)
    {
        for (k = 0; k < n-i; k++)
        {
            printf(" ");
        }
        for (j = n-i; j <= n+i; j++)
        {
            printf("%c", ch);
        }
        printf("\n");
        ch++;
    }
    return 0;
}
