#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
int main()
{
    int n, i, sum=0;
    printf("Enter n: ");
    scanf("%d", &n);
    
    int a[20];
    printf("Enter n numbers: \n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0; i<n; i++)
    {
       printf("%d ",a[i]);
       sum += a[i];
    }
    printf("\nSum is %d", sum);
    return 0;
}
