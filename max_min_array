#include <stdio.h>
#include <conio.h>
int main()
{
    int i, n=5, a[5]={1,5,8,0,-3};
    int max, min;
    max == min == a[0];
    for(i=0; i<n; i++)
    {
        if(a[i]>max) 
        {
            max = a[i];
        }
        else if (a[i]<min)
        {
            min = a[i];
        }
    }
    printf("max:%d and min:%d",max, min);
    return 0;
}
