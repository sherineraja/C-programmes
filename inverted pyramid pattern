#include <stdio.h>
#include <conio.h>

int main()
{
    int n = 5, i, j, k;
    
    for (i = 1; i <= n; i++)
    {
        for (j = 1; j <= i; j++)
        {
            printf(" ");
        }
        for (k = i; k <= (n+n-i); k++)
        {
            printf("*");
        }
        printf("\n");
    }
    
    return 0;
}
